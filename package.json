    "release": "standard-version",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "author": "vaanxy",
  "description": "Container terminal related UI components & tools based on Angular & d3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com:vaanxy/ng-container-terminal.git"
  },
  "dependencies": {
    "@angular-devkit/core": "^13.3.0",
    "@angular/animations": "^13.3.0",
