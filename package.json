{
  "name": "ng-container-terminal",
  "version": "0.7.7",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "pretty": "prettier --write --config ./.prettierrc ./projects/**/*.{ts,html,css,less}",
    "start": "ng serve",
    "build": "ng build ng-container-terminal --prod && cp README.md dist/ng-container-terminal && cp CHANGELOG.md dist/ng-container-terminal",
    "test": "ng test ng-container-terminal",
    "test-headless": "ng test ng-container-terminal --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "author": "vaanxy",
  "description": "Container terminal related UI components & tools based on Angular & d3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com:vaanxy/ng-container-terminal.git"
  },
  "dependencies": {
    "@angular/animations": "^10.2.5",
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^10.2.5",
    "@angular/core": "^10.2.5",
    "@angular/forms": "^10.2.5",
    "@angular/platform-browser": "^10.2.5",
    "@angular/platform-browser-dynamic": "^10.2.5",
    "@angular/router": "^10.2.5",
    "core-js": "^2.4.1",
    "d3": "^5.7.0",
    "rxjs": "^6.5.4",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular-devkit/build-ng-packagr": "~0.1002.3",
    "@angular/cli": "^10.2.3",
    "@angular/compiler-cli": "^10.2.5",
    "@types/d3": "^5.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "prettier": "1.15.3",
    "protractor": "~7.0.0",
    "standard-version": "^7.0.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
